<h1 class="page_title">訪客登入</h1>

<%= form_tag '/visitors/log_in' do %>
  <div class="form-group">
    帳號：<%= text_field_tag 'account',nil ,maxlength: 12 %><br />
  <div>
  <div class="form-group">
    密碼：<%= password_field_tag 'pw', nil, maxlength: 12 %><br />
  <div>
  <div class="form-group">
    <br><%= submit_tag "登入", class:"btn btn-primary btn-sm" %> <%= link_to '回訪客列表', visitors_path, class: "btn btn-default btn-sm" %>
  </div>
<% end %>
<%= @error%>
