<%= form_for(visitor) do |f| %>
  <%= f.label :account, "帳號 Account :" %>
  <%= f.text_field :account ,size: 18, maxlength: 12 %> <br />

  <%= f.label :pw, "密碼 Pw :" %>
  <%= f.password_field :pw ,size: 23, maxlength: 12%> <br />

  <%= f.label :name, "姓名 Name :" %>
  <%= f.text_field :name ,size: 20 ,maxlength: 10%> <br />

  <%= f.label :email, "信箱 Email :" %>
  <%= f.text_field :email, maxlength: 20 %> <br />

  <%= f.submit '送出資料' %>
<% end %>
